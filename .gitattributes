# 데이터 파일들을 Git LFS로 관리
data/** filter=lfs diff=lfs merge=lfs -text
*.db filter=lfs diff=lfs merge=lfs -text
*.json filter=lfs diff=lfs merge=lfs -text

# ChromaDB 관련 파일들
data/chroma_db_recall/** filter=lfs diff=lfs merge=lfs -text

# 대용량 JSON 파일들 (크롤링 결과)
**/realtime_recalls_*.json filter=lfs diff=lfs merge=lfs -text
**/fda_recalls_*.json filter=lfs diff=lfs merge=lfs -text